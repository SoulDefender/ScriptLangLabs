{% extends 'template.html.twig' %}

{% block content %}
    <div class="container">
        <div class="row">
            <div class="col-md-6 col-md-offset-3">Something went wrong...</div>
        </div>
        <div class="row">
            <div class="col-md-4 col-md-4">
                <h3>Error: {{ errorCode }}</h3>
            </div>
        </div>
        <div class="row">
            <div class="col-md-4 col-md-4">
                <h3>Message: {{ message }}</h3>
            </div>
        </div>
    </div><!-- /card-container -->

{% endblock %}